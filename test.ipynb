   "source": [
    "def latex_float(f):\n",
    "    float_str = \"{0: .2g}\".format(f)\n",
    "    if \"e\" in float_str:\n",
    "        base, exponent = float_str.split(\"e\")\n",
    "        return r\"${0} \\times 10^{{{1}}}$\".format(base, int(exponent))\n",
    "    else:\n",
    "        return float_str\n",
    "\n",
    "string = 1.23131e-5\n",
    "\n",
    "print(latex_float(string))"
   ]
